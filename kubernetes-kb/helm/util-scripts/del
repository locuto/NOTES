kubectl delete deployment kube-proxy --force --grace-period 0 -n kube-system
kubectl get deployments -n kube-system
kubectl delete ds kube-proxy --force --grace-period 0 -n kube-system
kubectl get ds -n kube-system
kubectl get pods -n kube-system|grep proxy
kubectl delete serviceaccount kube-proxy -n kube-system
kubectl delete ClusterRoleBinding kube-proxy -n kube-system
